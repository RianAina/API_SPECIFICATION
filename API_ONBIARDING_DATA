openapi: 3.0.0
info:
  title: API d'Onboarding Ajout de Données Texte
  version: 1.0.0
servers:
  - url: http://STD22074.com
paths:
  /students:
    get:
      summary: Insertion des eleves
      responses:
        '200':
          description: Succès 
          content:
            application/json:
              example:
                - id: 1
                  Donnée1: Rian'Aina
                  Donnée2: Rabearivelo
                - id: 2
                  Donnée1: Valery
                  Donnée2: Rabearivelo
        '500':
          description: Erreur 

    post:
summary: Création de nouvelles données
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: object
              properties:
                nom:
                  type: string
                prenom:
                  type: string
      responses:
        '200':
          description: Succès 
          content:
            application/json:
              example:
                id: 3
                Donnée1: John
                Donnée2: Doe
'500':
          description: Erreur
